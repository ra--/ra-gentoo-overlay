depend() {
	before device-mapper lvm
}

start() {
	ebegin "Starting dmeventd"
	start-stop-daemon --start --exec /sbin/dmeventd
	eend $?
}

stop() {
	ebegin "Stopping dmeventd"
	start-stop-daemon --stop --exec /sbin/dmeventd
	eend $?
}

